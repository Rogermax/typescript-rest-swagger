{
  "name": "typescript-rest-swagger",
  "version": "1.1.7",
  "description": "Generate Swagger files from a typescript-rest project",
  "keywords": [
    "typescript",
    "typescript-rest",
    "swagger",
    "open api",
    "rest",
    "microservice",
    "codegen",
    "generation"
  ],
  "main": "./dist/decorators.js",
  "typings": "./dist/decorators.d.ts",
  "scripts": {
    "start": "tsc -w",
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "lint": "eslint ./src/**/*.ts ./test/**/*.ts",
    "lint:fix": "eslint --fix ./src/**/*.ts ./test/**/*.ts",
    "swagger-gen": "node ./dist/cli.js -c ./test/data/swagger.js",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
    "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage --coverageReporters=text-lcov",
    "tsc": "tsc"
  },
  "author": "Thiago da Rosa de Bustamante <trbustamante@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/node": "*",
    "@typescript-eslint/parser": ">=5.0.0 <6.0.0",
    "argparse": "^1.0.10",
    "debug": "^4.3.4",
    "fs-extra-promise": "^1.0.1",
    "glob": "^7.2.3",
    "lodash": "^4.17.21",
    "merge": "^2.1.1",
    "minimatch": "^3.1.2",
    "mkdirp": "^1.0.4",
    "path": "^0.12.7",
    "swagger2openapi": "^7.0.8",
    "typescript": "^4.9.4",
    "typescript-rest": "^3.0.4",
    "webpack": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.38",
    "@types/debug": "^4.1.7",
    "@types/fs-extra-promise": "^1.0.10",
    "@types/glob": "^7.2.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.191",
    "@types/minimatch": "^3.0.5",
    "@types/mkdirp": "^1.0.2",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^27.5.1",
    "jsonata": "^1.8.6",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thiagobustamante/typescript-rest-swagger.git"
  },
  "bin": {
    "swaggerGen": "dist/cli.js"
  },
  "directories": {
    "lib": "dist",
    "doc": "doc"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "engineStrict": true
}
